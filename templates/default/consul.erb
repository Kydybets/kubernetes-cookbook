[Unit]
Description=Consul
After=network.service

[Service]
User=root
ExecStart=/usr/local/bin/consul agent -ui -server -client=<%=@node_addr%> -advertise=<%=@node_addr%> -ui-dir=/var/lib/consul/web --data-dir=/var/lib/consul --bootstrap-expect=1
Restart=always
RestartSec=10

[Install]
WantedBy=multi-user.target