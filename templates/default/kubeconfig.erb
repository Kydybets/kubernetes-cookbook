apiVersion: v1
kind: Config
preferences: {}
clusters:
- cluster:
    certificate-authority: <%=@base64encodedcacert%>
    server: <%=@cluster_url%>
  name: local
contexts:
- context:
    cluster: local
    user: kubelet
    namespace: kube-system
  name: service-account-context
current-context: service-account-context
users:
- name: kubelet
  user:
    client-certificate: <%=@base64encodedcert%>
    client-key: <%=@base64encodedkey%>