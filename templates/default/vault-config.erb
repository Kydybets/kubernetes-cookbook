cluster_name = "<%= @cluster_name %>"

listener "tcp" {
  address = "<%= @node_addr %>:8200"
  tls_cert_file = "<%= @server_certificate_file %>"
  tls_key_file = "<%= @server_key_file %>"
}

backend "consul" {
  address = "<%= @node_addr %>:8500"
  redirect_addr = "https://<%= @node_addr %>:8200"
  path = "vault"
}
